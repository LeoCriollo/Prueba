                if (result.ok) {
                    return result.json();
                }
            })
            .then(function (data) {

                cbo = document.getElementById("SlctDirectores1");
                cbo.innerHTML = "";
                data.forEach(function (element) {

                    let opt = document.createElement("option");
                    opt.appendChild(document.createTextNode(element.Text));
                    opt.value = element.Value;


                    cbo.appendChild(opt);
                })
            })
    }

