@model DoleEcIntranet.Models.ReclutamientoInternoViewModel


@{
    ViewBag.Title = "Reclutamiento Interno";
}



<section class="page-header">
    <div class="container">
        <div class="row">
            <div class="col">
                <ul class="breadcrumb">
                    <li>Personal</li>
                    <li>Selección y Desempeño</li>
                    

                </ul>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <h1>Reclutamiento Interno</h1>
            </div>
        </div>
    </div>
</section>

<div class="container">
    <div class="row">
        @if(Model != null)
        {
            <div class="col-lg-12">
                @Html.Raw(Model.Contenido)
            </div>

        }
        else
        {
            <div class="col-lg-12">
                <p class="text-center">No existe registros.</p>
            </div>
        }
    </div>
</div>


